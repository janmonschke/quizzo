{
  "private": true,
  "sideEffects": false,
  "prisma": {
    "seed": "ts-node --transpile-only prisma/seed.ts"
  },
  "scripts": {
    "build": "run-s \"build:*\"",
    "build:css": "npm run generate:css -- --minify",
    "build:remix": "remix build",
    "dev": "run-p \"dev:*\"",
    "dev:css": "npm run generate:css -- --watch",
    "dev:remix": "remix dev",
    "generate:css": "npx tailwindcss -o ./app/tailwind.css",
    "start": "remix-serve build",
    "purge_db": "rm prisma/dev.db && prisma db push && prisma db seed",
    "test": "jest"
  },
  "dependencies": {
    "@prisma/client": "^4.3.1",
    "@remix-run/node": "^1.6.8",
    "@remix-run/react": "^1.6.8",
    "@remix-run/serve": "^1.6.8",
    "classnames": "^2.3.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-movable": "^3.0.4"
  },
  "devDependencies": {
    "@babel/core": "^7.19.0",
    "@babel/preset-env": "^7.19.0",
    "@babel/preset-typescript": "^7.18.6",
    "@remix-run/dev": "^1.6.8",
    "@remix-run/eslint-config": "^1.6.8",
    "@types/jest": "^29.0.0",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "babel-jest": "^29.0.2",
    "eslint": "^8.20.0",
    "jest": "^29.0.2",
    "npm-run-all": "^4.1.5",
    "prisma": "^4.3.1",
    "tailwindcss": "^3.1.8",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  },
  "engines": {
    "node": ">=14"
  }
}
